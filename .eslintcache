  }\n            case INCREASE_QUANTITY:\n                state.numberCart++\n                state.Carts[action.payload].quantity++;\n               \n               return{\n                   ...state\n               }\n            case DECREASE_QUANTITY:\n                let quantity = state.Carts[action.payload].quantity;\n                if(quantity>1){\n                    state.numberCart--;\n                    state.Carts[action.payload].quantity--;\n                }\n               \n                return{\n                    ...state\n                }\n            case DELETE_CART:\n                let quantity_ = state.Carts[action.payload].quantity;\n                return{\n                    ...state,\n                    numberCart:state.numberCart - quantity_,\n                    Carts:state.Carts.filter(item=>{\n                        return item.id!==state.Carts[action.payload].id\n                    })\n                    \n                }\n        default:\n            return state;\n    }\n}\nconst ShopApp = combineReducers({\n    _todoProduct:todoProduct\n});\nexport default ShopApp;","/Users/osvaldo.amador/Dev/test-shoppingcart-ecomsur/src/styles/components.js",[],"/Users/osvaldo.amador/Dev/test-shoppingcart-ecomsur/src/styles/index.js",[],"/Users/osvaldo.amador/Dev/test-shoppingcart-ecomsur/src/components/Layout.js",[],{"ruleId":"61","replacedBy":"62"},{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","severity":1,"message":"66","line":7,"column":5,"nodeType":"67","messageId":"68","endLine":9,"endColumn":6},{"ruleId":"69","severity":1,"message":"70","line":25,"column":33,"nodeType":"71","endLine":25,"endColumn":60},{"ruleId":"72","severity":1,"message":"73","line":42,"column":33,"nodeType":"71","endLine":42,"endColumn":123},{"ruleId":"72","severity":1,"message":"73","line":43,"column":33,"nodeType":"71","endLine":43,"endColumn":61},{"ruleId":"72","severity":1,"message":"73","line":44,"column":33,"nodeType":"71","endLine":44,"endCo